<body>
<!-- Spritre -->
	<!-- @import "sprite.html" -->
<!-- Spritre -->
<!-- header -->
	<!-- @import "header.html" -->
<!-- header -->

<!-- headerpink -->
<section class="headerpink">
	<div class="container">
		<div class="headerpink__inner">
			<h3 class="headerpink__title">SHOPPING CART</h3>
		</div>
	</div>
</section>
<!-- headerpink -->
<!-- cart -->
<section class="cart">
	<div class="container">
		<div class="cart__inner">
			<div class="cart__cards">
				<div class="cartCards__item">
					<img class="cartCards__img" src="img/cart/cart1.png" alt="">
					<div class="cartCards__content">
						<h4 class="cartCards__title">MANGO  PEOPLE  T-SHIRT</h4>
						<p class="cartCards__feature">Price:<span class="price">$300</span></p>
						<p class="cartCards__feature">Color:<span>Red</span></p>
						<p class="cartCards__feature">Size:<span>XL</span></p>
						<p class="cartCards__feature">Quantity:<span class="quantity">2</span></p>
					</div>
				</div>

				<div class="cartCards__item">
					<img class="cartCards__img" src="img/cart/cart2.png" alt="">
					<div class="cartCards__content">
						<h4 class="cartCards__title">MANGO  PEOPLE  T-SHIRT</h4>
						<p class="cartCards__feature">Price:<span class="price">$300</span></p>
						<p class="cartCards__feature">Color:<span>Red</span></p>
						<p class="cartCards__feature">Size:<span>XL</span></p>
						<p class="cartCards__feature">Quantity:<span class="quantity">2</span></p>
					</div>
				</div>
				<div class="cart__button">
					<button class="cart--btn">CLEAR SHOPPING CART</button>
					<button class="cart--btn">CONTINUE SHOPPING</button>
				</div>
			</div>

			<div class="cart__form">
				<div>
				<form class="cart__form" action="">
					<h3 class="cart__formTitle">SHIPPING ADRESS</h3>
					<input class="cart__input" type="text" placeholder="Bangladesh">
					<input class="cart__input" type="text" placeholder="State">
					<input class="cart__input" type="text" placeholder="Postcode / Zip">
					<button class="cart__formBtn">GET A QUOTE</button>
				</form>
				</div>
				<div class="cart__total">
					<p class="total__suptext">SUB TOTAL $900</p>
					<p class="total__subtext">GRAND TOTAL <span>$900</span></p>

					<button class="cart__totalbtn">PROCEED TO CHECKOUT</button>
				</div>
			</div>
		</div>
	</div>
</section>
<!-- cart -->

